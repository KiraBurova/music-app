{
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest --coverage --watchAll"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/**/*.tsx",
      "!<rootDir>/pages/*.tsx",
      "!<rootDir>/pages/**/*.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "html",
      "text"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "testEnvironment": "jsdom"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.1.2",
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "babel-jest": "^29.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "cypress": "^10.7.0",
    "effector": "^22.3.0",
    "effector-next": "^1.3.3",
    "effector-react": "^22.1.6",
    "fetch-jsonp": "^1.2.3",
    "lodash": "^4.17.21",
    "next": "^12.0.0",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.54.9"
  }
}
